pipeline{
    agent any
    tools{
        jdk 'JDK'
        maven 'Maven'
    }
    stages {
        stage ('PULL THE APPLICATION FROM GITHUB') {
            steps{
                git branch: 'ci-jenkins', url: 'https://github.com/goksmann/personal.git'
            }
        }
        stage ('BUILD THE APPLICATION') {
            steps {
                sh "mvn install -DeskipTest'
            }
        }
            }
        }
    }
}