pipeline{
    agent any
    tools{
        jdk 'JDK'
        maven 'Maven'
    } 
    stages{
        stage('GIT CHECK'){
            steps{
                git branch: 'ci-jenkins', url: 'https://github.com/goksmann/personal.git'
            }
        } 
        stage('UNIT TEST'){
            steps{
                sh 'mvn test'
            }
        }  
        stage('MAVEN BUILD'){
            steps{
                sh 'mvn clean install'
            }
        }
    }  
}